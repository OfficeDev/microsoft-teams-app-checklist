{
    "name": "checklist",
    "version": "1.0.0",
    "description": "Checklist",
    "scripts": {
        "build": "webpack --env.mode=prod --env.watch=false",
        "start": "webpack --env.mode=dev --env.watch=true",
        "zip": "cd ./output && bestzip ./ActionPackage.zip *",
        "deploy": "npm run build && npm run zip && upload-action-package -z ./output/ActionPackage.zip -t ./output/ --ld ./output/ --lp checklist",
        "deploy-debug": "npm run build && npm run zip && upload-action-package -z ./output/ActionPackage.zip -t ./output/ -l debug --ld ./output/ --lp checklist",
        "update-teams-app": "npm run build && npm run zip && upload-action-package -z ./output/ActionPackage.zip --ld ./output/ --lp checklist",
        "inner-loop": "create-inner-loop -p <packageId> -d ./output --ld ./output/ --lp checklist",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix"
    },
    "dependencies": {
        "@fluentui/react-northstar": "0.51.0",
        "@microsoft/m365-action-sdk": "https://static.actions.office365.com/sdk/v1.0.4/microsoft-m365-action-sdk.tgz",
        "css-loader": "2.1.1",
        "html2canvas": "1.0.0-rc.5",
        "mobx": "5.9.4",
        "mobx-react": "5.1.2",
        "office-ui-fabric-react": "7.22.0",
        "react": "16.8.0",
        "react-dom": "16.8.0",
        "recyclerlistview": "3.0.0",
        "satcheljs": "4.0.1",
        "style-loader": "0.21.0",
        "uuid": "8.1.0"
    },
    "devDependencies": {
        "@types/react": "16.8.0",
        "@types/react-dom": "16.8.0",
        "action-package-deploy": "1.0.22",
        "bestzip": "2.1.7",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "6.1.0",
        "html-webpack-plugin": "3.2.0",
        "node-sass": "4.14.1",
        "sass-loader": "7.1.0",
        "ts-loader": "5.4.5",
        "tslint": "6.1.3",
        "tslint-microsoft-contrib": "6.2.0",
        "typescript": "3.5.3",
        "webpack": "4.37.0",
        "webpack-cli": "3.3.12"
    }
}
